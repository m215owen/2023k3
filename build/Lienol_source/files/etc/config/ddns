
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'aali'
	option service_name 'aliyun.com'
	option enabled '1'
	option lookup_host 'www.m216owen.top'
	option domain 'www.m216owen.top'
	option username 'LTAI5t7QN44tkLcZZ1nEXpkU'
	option password 'tve1NUd3l7S68zK7SLvi4PM4O40Qch'
	option ip_source 'interface'
	option interface 'wan'
	option ip_interface 'pppoe-wan'
	option force_ipversion '1'

config service 'aaliipv6'
	option use_ipv6 '1'
	option service_name 'aliyun.com'
	option lookup_host 'www.m216owen.top'
	option domain 'www.m216owen.top'
	option username 'LTAI5t7QN44tkLcZZ1nEXpkU'
	option password 'tve1NUd3l7S68zK7SLvi4PM4O40Qch'
